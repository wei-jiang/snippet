//16.04
letsencrypt certonly --webroot \
    -w /var/www/ssl-proof/ \
    --agree-tos --email ww@wwpp.date \
    -d wwpp.date


wget https://dl.eff.org/certbot-auto
chmod a+x certbot-auto

./certbot-auto certonly --webroot \
    -w /var/www/ssl-proof/ \
    --agree-tos --email david@cninone.com \
    -d spp.yunyougo.com   

./certbot-auto certonly --webroot \
    -w /var/www/ssl-proof/ \
    --agree-tos --email david@cninone.com \
    -d buy.nanyue.net.cn

